{
  "name": "only-utils",
  "version": "0.6.2",
  "license": "MIT",
  "sideEffects": false,
  "description": "Only Nice Lean Yare - UTILS",
  "repository": {
    "type": "git",
    "url": "https://github.com/kacper-olszanski/only-utils.git"
  },
  "type": "module",
  "main": "dist/only-utils.umd.js",
  "module": "dist/only-utils.js",
  "types": "dist/only-utils.d.ts",
  "exports": {
    "types": "./dist/only-utils.d.ts",
    "import": "./dist/only-utils.js",
    "require": "./dist/only-utils.umd.js"
  },
  "author": {
    "name": "Kacper Olszanski",
    "email": "dev@rvtr.media",
    "url": "https://github.com/kacper-olszanski/"
  },
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "util",
    "utility",
    "types",
    "modules",
    "es",
    "ts"
  ],
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc --p ./tsconfig.build.json && vite build",
    "dev": "vite",
    "preview": "vite preview",
    "upi": "pnpm update --interactive",
    "lint": "eslint . --ext ts",
    "lint:fix": "eslint --fix .",
    "test": "vitest",
    "treecheck": "pnpm build && agadoo dist/only-utils.js",
    "cs": "pnpm changeset",
    "cs:version": "pnpm cs version",
    "release": "pnpm build && pnpm gen:docs && pnpm cs publish",
    "gen:docs": "node ./tooling/gen-docs.cjs",
    "prepare": "husky"
  },
  "peerDependencies": {
    "@only/types": "npm:@jsr/only__types@>=0.1.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.9",
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "@vitest/coverage-v8": "^2.1.2",
    "agadoo": "^3.0.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "8.10.0",
    "eslint-config-xo": "^0.45.0",
    "eslint-config-xo-typescript": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.5",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "pretty-quick": "^4.0.0",
    "typedoc": "^0.26.8",
    "typedoc-plugin-markdown": "^4.2.9",
    "typescript": "^5.6.3",
    "vite": "^5.4.8",
    "vite-plugin-dts": "^3.9.1",
    "vitest": "^2.1.2"
  },
  "dependencies": {
    "boolchain": "^0.3.3"
  },
  "overrides": {
    "boolchain": {
      "only-utils": "0.6.2"
    }
  }
}
